{
  "name": "kollectionjs",
  "version": "0.0.0",
  "description": "Collection utilities inspired by kotlin for javascript",
  "repository": "https://geowarin/kollectionjs",
  "author": "Geoffroy Warin",
  "license": "MIT",
  "private": false,
  "browser": "dist/kollection.umd.es6.js",
  "main": "dist/kollection.cjs.js",
  "module": "dist/kollection.esm.js",
  "scripts": {
    "test": "jest",
    "build": "rollup -c"
  },
  "dependencies": {
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "jest": "^21.2.1",
    "rollup": "^0.52.1",
    "rollup-plugin-typescript2": "^0.8.4",
    "ts-jest": "^21.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.(ts|tsx)"
    ],
    "cacheDirectory": "./.cache/jest-cache",
    "modulePaths": [
      "src/main"
    ]
  }
}
