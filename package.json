{
  "name": "kollectionjs",
  "version": "1.0.0",
  "description": "Collection utilities inspired by kotlin for javascript",
  "repository": "https://geowarin/kollectionjs",
  "author": "Geoffroy Warin",
  "license": "MIT",
  "private": false,
  "browser": "dist/kollection.umd.js",
  "main": "dist/kollection.js",
  "module": "dist/kollection.esm.js",
  "typings": "dist/index",
  "scripts": {
    "test": "jest",
    "build": "microbundle"
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "jest": "^23.2.0",
    "microbundle": "^0.4.4",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.(ts|tsx)"
    ],
    "cacheDirectory": "./.cache/jest-cache",
    "modulePaths": [
      "src/main"
    ]
  }
}
