{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015"],
    "rootDir": "src",
    "outDir": "lib",
    "downlevelIteration": true,
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "removeComments": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitThis": true,
    "noImplicitAny": true
  }
}