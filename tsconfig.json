{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es2015"
    ],
    "rootDir": "src",
    "outDir": "lib",
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "removeComments": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitThis": true,
    "noImplicitAny": true
  },
  "exclude": ["test"]
}